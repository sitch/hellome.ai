{
  "$schema": "https://json.schemastore.org/package",
  "private": true,
  "name": "hellome.ai",
  "appName": "HelloMe.ai",
  "appShortName": "HelloMe.ai",
  "appSubtitle": "Personalized children's books using AI",
  "appMetaDescription": "Personalized children's books using AI",
  "appDescription": "Personalized children's books using AI",
  "version": "1.0.2",
  "author": {
    "name": "Sitch",
    "email": "sitch@hellome.ai",
    "url": "https://github.com/sitch"
  },
  "_workspaces": [
    "packages/*"
  ],
  "browser": {
    "@prisma/client": false,
    "prisma": false
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts"
  },
  "scripts": {
    "postinstall": "yarn db.generate --generator client",
    "predev": "run-s --print-label favicons.generate.build",
    "dev": "nodemon --config nodemon.json --exec next dev",
    "dev.inspect": "nodemon --config nodemon.json --exec env NODE_OPTIONS='--inspect' next dev",
    "prebuild": "run-s --print-label timestamps.generate favicons.generate.build routes.generate && yarn db.generate --generator client",
    "build": "next build",
    "postbuild": "run-s --print-label sitemap.generate",
    "start": "next start",
    "build.ci": "env NODE_ENV=test yarn build",
    "start.ci": "env NODE_ENV=test yarn start",
    "analyze": "env ANALYZE=true next build",
    "analyze.source.html": "source-map-explorer .next/**/*.js --html result.html && open result.html",
    "analyze.source.tsv": "source-map-explorer .next/**/*.js --tsv | column -t -s $'\t' | bat -l tsv",
    "analyze.source.json": "source-map-explorer .next/**/*.js --json",
    "db.build": "run-s --print-label db.format db.clean db.generate",
    "db.clean": "rimraf src/@gen/crud docs/prisma resources/diagrams/erd src/@gen/trpc src/@gen/zod",
    "db.format": "prisma format",
    "db.deploy": "prisma migrate deploy",
    "db.migrate": "prisma migrate dev --skip-seed",
    "db.reset": "prisma migrate reset --skip-seed",
    "db.push": "prisma db push --skip-seed",
    "db.seed": "prisma db seed",
    "db.studio": "prisma studio",
    "db.reset.hard": "rimraf prisma/migrations && run-s --silent --print-label db.build db.reset.hard.dev",
    "db.reset.hard.all": "rimraf prisma/migrations && run-s --silent --print-label db.build db.reset.hard.dev db.reset.hard.prod",
    "db.reset.hard.dev": "dotenv -e .env -- sh -c 'yarn db.reset --force && yarn db.migrate --name create_schema && yarn db.seed'",
    "db.reset.hard.prod": "dotenv -e prisma/.env.production -- sh -c 'yarn db.reset --force && yarn db.migrate --name create_schema && yarn db.seed'",
    "db.generate": "mkdir -p resources/diagrams/erd && yarn prisma generate",
    "zenstack.generate": "rimraf ./src/@gen/zenstack && mkdir -p resources/diagrams/erd && yarn zenstack generate",
    "favicons.generate_": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' bin/generate-favicons.ts",
    "favicons.generate.build": "node bin/generate-favicons.mjs",
    "favicons.generate": "node bin/generate-favicons.mjs --force",
    "timestamps.generate": "node bin/generate-file-modified-timestamps.mjs",
    "sitemap.generate": "next-sitemap",
    "mailing.dev": "npx mailing",
    "mailing.build": "npx mailing server build",
    "mailing.start": "npx mailing server start",
    "__________openapi.build__openapi.validate_fails": "run-s --print-label openapi.validate openapi.format openapi.clean openapi.generate",
    "openapi.build": "run-s --print-label openapi.format openapi.clean openapi.generate",
    "openapi.generate": "env TS_POST_PROCESS_FILE='bin/openapi-post-process.sh' openapi-generator-cli generate",
    "openapi.clean": "rimraf ./src/@gen/lulu",
    "openapi.format": "eslint openapi.lulu.yml --fix",
    "openapi.validate": "openapi-generator-cli validate --input-spec openapi.lulu.yml",
    "typecheck:tsc": "tsc --noemit --project tsconfig.json",
    "typecheck:tsc.cypress": "tsc --noemit --project cypress/tsconfig.json",
    "pretypecheck": "yarn routes.generate",
    "typecheck": "run-s --silent --print-name typecheck:*",
    "lint:alex": "alex . --quiet --why",
    "lint:cspell": "cspell lint --no-progress --cache --cache-location .cspellcache ./ || true",
    "lint:next": "next lint --cache-location .eslintcache",
    "lint:cypress": "eslint --config cypress/.eslintrc.js",
    "lint:browserslist": "npx browserslist-lint",
    "lint": "run-s --silent --print-name typecheck lint:*",
    "lint-fix:next": "next lint --cache-location .eslintcache --fix",
    "lint-fix:cypress": "eslint --config cypress/.eslintrc.js --fix",
    "lint.fix": "run-s --silent --print-name lint:browserslist lint:cspell lint:alex lint-fix:*",
    "format:prettier": "prettier --loglevel warn --cache --cache-location=.prettiercache --write ./",
    "format": "run-s --silent --print-name format:*",
    "clean:cspell": "rimraf .cspellcache",
    "clean:eslint": "rimraf eslintcache",
    "clean:next": "rimraf .next .swc",
    "clean:next-pwa": "rimraf public/fallback-*.js public/sitemap*.xml public/sw.js public/workbox-*.js",
    "__________clean:nextjs-routes": "rimraf nextjs-routes.d.ts",
    "clean:node_modules": "rimraf node_modules",
    "clean:prettier": "rimraf .prettiercache",
    "clean:tsc": "tsc --build --clean",
    "clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo out",
    "clean:yarn": "rimraf .yarn yarn-error.log package-lock.json",
    "clean": "run-p --silent --print-name clean:** && yarn cache clean && yarn --check-files && yarn gen",
    "_________routes.generate": "npx nextjs-routes",
    "routes.generate": "echo '[routes.generate] skipping...'",
    "mesh.generate:character": "npx gltfjsx public/@meshes/character.glb --types --shadows --transform --resolution 1024 --root public/@meshes --output src/@gen/meshes/CharacterMesh.tsx",
    "mesh.generate:book": "npx gltfjsx public/@meshes/book.glb --types --shadows --transform --resolution 1024 --root public/@meshes --output src/@gen/meshes/BookMesh.tsx",
    "mesh.generate:book2": "npx gltfjsx public/@meshes/book2.glb --types --shadows --transform --resolution 1024 --root public/@meshes --output src/@gen/meshes/Book2Mesh.tsx",
    "mesh.generate": "mkdir -p src/@gen/meshes && run-p --print-label mesh.generate:* && mv *.glb public/@meshes/",
    "gen.all": "run-p --print-label openapi.build gen",
    "gen": "run-s --print-label mesh.generate timestamps.generate favicons.generate db.build routes.generate",
    "test": "jest",
    "test.ci": "jest --ci",
    "test.fix": "jest --updateSnapshot",
    "test.watch": "jest --watch",
    "e2e": "yarn build.ci && start-server-and-test 'yarn start.ci' http://127.0.0.1:3000 'yarn cy.run'",
    "cy.run": "env NODE_OPTIONS=--openssl-legacy-provider yarn cypress run --config-file cypress/cypress.config.ts",
    "cy.open": "env NODE_OPTIONS=--openssl-legacy-provider yarn cypress open --config-file cypress/cypress.config.ts",
    "ci": "run-s --silent typecheck lint test.ci",
    "fix": "run-s --silent format typecheck lint.fix test.fix",
    "up.check": "ncu",
    "up.pkg": "ncu -u",
    "up": "yarn upgrade-interactive --latest",
    "unused:libs": "depcheck --detailed",
    "unused:files": "next-unused",
    "unused": "run-p unused:*"
  },
  "sideEffects": false,
