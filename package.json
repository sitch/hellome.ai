{
  "name": "hellome.ai",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "nodemon --config next.nodemon.json --exec next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "mailing.dev": "npx mailing",
    "mailing.build": "npx mailing server build",
    "mailing.start": "npx mailing server start",
    "typecheck:tsc": "tsc --noemit --project tsconfig.json",
    "typecheck:tsc.cypress": "tsc --noemit --project cypress/tsconfig.json",
    "typecheck": "run-s --silent --print-name typecheck:*",
    "lint:cspell": "cspell --no-progress --cache --cache-location .cspellcache ./",
    "lint:next": "next lint --cache-location .eslintcache",
    "lint:cypress": "eslint --config cypress/.eslintrc.js",
    "lint": "run-s --silent --print-name lint:*",
    "lint-fix:next": "next lint --cache-location .eslintcache --fix",
    "lint-fix:cypress": "eslint --config cypress/.eslintrc.js --fix",
    "lint.fix": "run-s --silent --print-name lint:cspell lint-fix:*",
    "format:prettier": "prettier --loglevel warn --cache --cache-location=.prettiercache --write ./",
    "format": "run-s --silent --print-name format:*",
    "clean": "run-p --silent --print-name clean:**",
    "test": "jest",
    "test.ci": "jest --ci",
    "test.fix": "jest --updateSnapshot",
    "test.watch": "jest --watch",
    "e2e": "start-server-and-test dev http://localhost:3000 cy.run",
    "cy.run": "env NODE_OPTIONS=--openssl-legacy-provider yarn cypress run --config-file cypress/cypress.config.ts",
    "cy.open": "env NODE_OPTIONS=--openssl-legacy-provider yarn cypress open --config-file cypress/cypress.config.ts",
    "ci": "run-s --silent typecheck lint test.ci",
    "fix": "run-s --silent format typecheck lint.fix test.fix",
    "up": "yarn upgrade-interactive --latest",
    "find:unused": "npx next-unused"
  },
  "sideEffects": false,
  "dependencies": {
    "@next/font": "13.1.6",
    "@stdlib/datasets-female-first-names-en": "^0.0.9",
    "@stdlib/datasets-male-first-names-en": "^0.0.9",
    "@vercel/analytics": "^0.1.10",
    "classnames": "^2.3.2",
    "clsx": "^1.2.1",
    "dotenv": "^16.0.3",
    "encoding": "^0.1.13",
    "flowbite": "^1.6.3",
    "flowbite-react": "^0.3.8",
    "lodash": "^4.17.21",
    "mailing-core": "^0.9.10",
    "mjml": "^4.13.0",
    "mjml-react": "^2.0.8",
    "next": "13.1.6",
    "nextjs-google-analytics": "^2.3.0",
    "nodemailer": "^6.9.1",
    "nodemon": "^2.0.20",
    "postcss-nesting": "^11.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.1",
    "reaptcha": "^1.12.1"
  },
  "dependencies.todo": {
    "flowbite": "^1.6.3",
    "flowbite-react": "^0.3.8"
  },
  "devDependencies": {
    "@downwindcss/easings": "^1.0.4",
    "@frsource/cypress-plugin-visual-regression-diff": "^3.2.8",
    "@next/bundle-analyzer": "^13.1.6",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/lodash": "^4.14.191",
    "@types/mjml-react": "^2.0.6",
    "@types/node": "18.13.0",
    "@types/nodemailer": "^6.4.7",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/testing-library__jest-dom": "^5.14.5",
    "autoprefixer": "^10.4.13",
    "cspell": "^6.25.0",
    "cypress": "^12.6.0",
    "cypress-react-selector": "^3.0.0",
    "eslint": "8.34.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-testing-library": "^5.10.1",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "lint-staged": "^13.1.2",
    "mailing": "^0.9.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-easings": "^3.0.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "start-server-and-test": "^1.15.4",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss": "^3.2.6",
    "ts-jest": "^29.0.5",
    "typescript": "4.9.5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": [
    "chrome >= 90",
    "edge >= 90",
    "firefox >= 100",
    "android >= 90",
    "safari >= 15"
  ]
}
